# Ambiente
NODE_ENV=production

# API Configuration
VITE_API_URL=https://apiblogpost.onrender.com/v1

# Database (se usar PostgreSQL)
POSTGRES_DB=edublog
POSTGRES_USER=edublog_user
POSTGRES_PASSWORD=your_secure_password

# Redis (se usar)
REDIS_URL=redis://redis:6379

# JWT (se implementar autenticação própria)
JWT_SECRET=your_jwt_secret_key

# Portas
FRONTEND_PORT=3000
DB_PORT=5432
REDIS_PORT=6379